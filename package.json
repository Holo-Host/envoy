{
  "name": "@holo-host/envoy",
  "description": "The go-between for all Holo-Hosting functionality",
  "version": "0.0.1",
  "main": "lib/index.js",
  "homepage": "https://github.com/holo-host/envoy",
  "license": "GPL-3",
  "scripts": {
    "start": "LOG_LEVEL=silly ts-node src/index.ts",
    "deps": "LOG_LEVEL=debug ts-node src/scripts/download-deps.ts",
    "build": "tsc -d",
    "watch": "tsc -p tsconfig.test.json -w ",
    "prod": "npm run build && node lib/index.js",
    "clean": "rm -fr lib",
    "init": "LOG_LEVEL=debug ts-node -T init-conductor.ts",
    "keygen": "ts-node -T keygen-cached.ts",
    "conductor": "holochain -c ~/.holochain/holo/conductor-config.toml",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "ENVOY_TEST=1 ts-node test/unit | faucet",
    "test:unit:raw": "ENVOY_TEST=1 ts-node test/unit",
    "test:integration": "ts-node test/integration | faucet",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "build-conductor-config": "node scripts/build-conductor-config.js"
  },
  "dependencies": {
    "@whi/stdlog": "^0.2.1",
    "archiver": "^3.0.0",
    "axios": "^0.18.1",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "extract-zip": "^1.6.7",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "rpc-websockets": "github:maackle/rpc-websockets",
    "static-server": "^2.2.1"
  },
  "devDependencies": {
    "@babel/runtime": "^7.4.3",
    "@holo-host/hclient": "^0.3.0",
    "@holochain/hc-web-client": "^0.5.0",
    "@types/node": "^11.9.4",
    "commander": "^2.19.0",
    "faucet": "^0.0.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.4",
    "tape": "^4.10.0",
    "tape-promise": "^4.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
